[configuration]

entry_symbol = "sercomm_library_init"
compatibility_minimum = "4.1"

[libraries]

macos.debug = "res://bin/libsercomm.macos.template_debug.framework"
macos.release = "res://bin/libsercomm.macos.template_release.framework"

windows.debug.x86_32 = "res://bin/libsercomm.windows.template_debug.x86_32.dll"
windows.release.x86_32 = "res://bin/libsercomm.windows.template_release.x86_32.dll"
windows.debug.x86_64 = "res://bin/libsercomm.windows.template_debug.x86_64.dll"
windows.release.x86_64 = "res://bin/libsercomm.windows.template_release.x86_64.dll"

linux.debug.x86_64 = "res://bin/libsercomm.linux.template_debug.x86_64.so"
linux.release.x86_64 = "res://bin/libsercomm.linux.template_release.x86_64.so"
linux.debug.arm64 = "res://bin/libsercomm.linux.template_debug.arm64.so"
linux.release.arm64 = "res://bin/libsercomm.linux.template_release.arm64.so"
linux.debug.rv64 = "res://bin/libsercomm.linux.template_debug.rv64.so"
linux.release.rv64 = "res://bin/libsercomm.linux.template_release.rv64.so"

[dependencies]
macos.debug = "res://bin/libserialport.framework"
macos.release = "res://bin/libserialport.framework"

windows.debug.x86_32 = "res://bin/libserialport.dll"
windows.release.x86_32 = "res://bin/libserialport.dll"
windows.debug.x86_64 = "res://bin/libserialport.dll"
windows.release.x86_64 = "res://bin/libserialport.dll"

linux.debug.x86_64 = "res://bin/libserialport.so"
linux.release.x86_64 = "res://bin/libserialport.so"
linux.debug.arm64 = "res://bin/libserialport.so"
linux.release.arm64 = "res://bin/libserialport.so"
linux.debug.rv64 = "res://bin/libserialport.so"
linux.release.rv64 = "res://bin/libserialport.so"